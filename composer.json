{
  "name": "paytic/omnipay-librapay",
  "type": "library",
  "license": "MIT",
  "description": "librapay driver for ThePhpLeague Omnipay multi-payment processing library",
  "keywords": [
    "librapay",
    "driver",
    "gateway",
    "merchant",
    "pay",
    "payment"
  ],
  "homepage": "https://github.com/Paytic/omnipay-librapay",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Paytic\\Omnipay\\Librapay\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Paytic\\Omnipay\\Librapay\\Tests\\": "tests/src",
      "Paytic\\Omnipay\\Librapay\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0|^8.0",
    "bytic/omnipay-common": "^3.0",
    "bytic/utility": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.2|^7.0|^8.0|^9.0",
    "vlucas/phpdotenv": "^2.5",
    "omnipay/tests": "^3",
    "nyholm/psr7": "^1.1"
  }
}
